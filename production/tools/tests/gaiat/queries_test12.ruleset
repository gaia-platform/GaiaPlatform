// RUN: not %gaiat %s -- 2>&1| FileCheck  %s

// TESTCASE: use a field name in the place of a table name in a path
ruleset test43
{
    on_insert(animal)
    {
        animal->feeding->portion = 5;
    }

}

// CHECK: Field 'portion' cannot be used after a 'refers to' operator ('->') in a path. Use a dot ('.') to separate the table name from the field name.
