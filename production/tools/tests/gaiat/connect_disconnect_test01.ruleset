// TESTCASE: connect/disconnect are not supported for VLR links
ruleset test_connect_child_parent_vlr_1_n
{
    on_insert(registration)
    {
        auto s = student.insert(student_id: "2", surname: "test");
        registration.connect(s);
    }
};

