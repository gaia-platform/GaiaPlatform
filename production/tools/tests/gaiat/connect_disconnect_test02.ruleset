// RUN: not %gaiat %s -- 2>&1| FileCheck  %s

// TESTCASE: connect/disconnect are not supported for VLR links
ruleset test_disconnect_child_parent_vlr_1_n
{
    on_insert(student)
    {
        for(/r:registration)
        {
            r.disconnect(student);
        }
    }
};

