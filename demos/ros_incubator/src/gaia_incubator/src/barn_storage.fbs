namespace BarnStorage;

table incubator {
      name: string;
      min_temp: float;
      max_temp: float;
}

table sensor {
      incubator_id: ulong;
      name: string;
}

table sensor_data {
      incubator_id: ulong;
      name: string;
      timestamp: ulong;
      value: float;
}

table fan {
      incubator_id: ulong;
      name: string;
}
