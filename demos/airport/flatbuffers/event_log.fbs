namespace gaia.rules;

table event_log {
    // system fields
    gaia_id: int64;
    // domain fields
    gaia_type : ulong;
    event_type : uint;
    event_mode : ubyte;
    event_source : string;
    timestamp : ulong;
    context_id : ulong;
    rules_fired : bool;
}
